{
  "name": "@coldcopy/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern='\\.(test|spec)\\.[jt]sx?$' --testPathIgnorePatterns='e2e|integration'",
    "test:integration": "jest --testPathPattern='integration.*\\.(test|spec)\\.[jt]sx?$'",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:db:setup": "tsx __tests__/setup/test-db-setup.ts",
    "test:db:clean": "tsx __tests__/setup/test-db-setup.ts clean",
    "test:load": "cd tests/load && k6 run run-tests.js",
    "test:load:light": "cd tests/load && TEST_TYPE=light k6 run run-tests.js",
    "test:load:moderate": "cd tests/load && TEST_TYPE=moderate k6 run run-tests.js",
    "test:load:heavy": "cd tests/load && TEST_TYPE=heavy k6 run run-tests.js",
    "test:load:spike": "cd tests/load && TEST_TYPE=spike k6 run run-tests.js",
    "test:load:soak": "cd tests/load && TEST_TYPE=soak k6 run run-tests.js",
    "test:load:email": "cd tests/load && k6 run email-load.js",
    "test:load:api": "cd tests/load && k6 run api-load.js",
    "test:load:database": "cd tests/load && k6 run database-load.js",
    "test:load:billing": "cd tests/load && k6 run billing-load.js",
    "test:load:gdpr": "cd tests/load && k6 run gdpr-load.js",
    "test:load:ci": "cd tests/load && BASE_URL=${BASE_URL:-http://localhost:3000} TEST_TYPE=light k6 run run-tests.js",
    "env:verify": "tsx scripts/verify-env.ts",
    "env:generate": "./scripts/generate-secrets.sh",
    "deploy:verify": "npm run env:verify && npm run build && npm run typecheck && npm run lint",
    "deploy:production": "vercel --prod",
    "verify:deployment": "tsx scripts/verify-deployment.ts",
    "verify:quick": "./scripts/quick-verify.sh",
    "test:prod": "playwright test tests/e2e/production-tests.spec.ts",
    "diagnose": "node scripts/diagnose-deployment.js",
    "monitor": "ts-node scripts/monitor-production.ts",
    "test:all": "./scripts/test-production.sh",
    "test:comprehensive": "ts-node scripts/run-production-test.ts",
    "verify:full": "npm run diagnose && npm run test:prod",
    "setup:tools": "npm install playwright tesseract.js pixelmatch pngjs commander && npx playwright install",
    "verify:tools": "node scripts/verify-tools.js"
  },
