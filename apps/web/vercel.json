{
  "crons": [
    {
      "path": "/api/billing/trial",
      "schedule": "0 9 * * *"
    }
  ],
  "functions": {
    "app/api/billing/trial/route.ts": {
      "maxDuration": 60
    },
    "app/api/email/send/route.ts": {
      "maxDuration": 30
    },
    "app/api/enrichment/process/route.ts": {
      "maxDuration": 300
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}